digraph {
// rankdir = LR; //order things from left to right

x [shape=circle];
d [shape=circle];
y [shape=circle];
y_ [shape=circle, label=<y<SUB>pred</SUB>>];
F [shape=rectangle, label=<F<SUB>&beta;</SUB>>];
